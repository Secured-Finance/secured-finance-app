# Local Secrets Template
# Copy this file to .env.local and fill with your API keys
# Never commit .env.local to Git
#
# Public defaults are defined in .env (committed to Git)

# Required
NEXT_PUBLIC_WALLET_CONNECT_ID=your_project_id  # https://cloud.walletconnect.com/

# RPC Providers (at least one recommended)
NEXT_PUBLIC_ALCHEMY_API_KEY=                   # https://alchemy.com/
NEXT_PUBLIC_ANKR_API_KEY=                      # https://ankr.com/

# Optional
NEXT_PUBLIC_GOOGLE_ANALYTICS_TAG=              # G-XXXXXXXXXX
NEXT_PUBLIC_AMPLITUDE_API_KEY=                 # Analytics (expensive, consider GA only)
NEXT_PUBLIC_SQUID_WIDGET_INTEGRATOR_ID=        # https://docs.squidrouter.com/
NEXT_PUBLIC_SUBGRAPH_URL_314=                  # Graph Protocol endpoint