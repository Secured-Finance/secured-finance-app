{
  "typescript.tsdk": "node_modules/typescript/lib",
  "files.associations": {
    "*.jsx": "javascriptreact",
    "*.tsx": "typescriptreact",
    "*.css": "tailwindcss"
  },
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "[javascriptreact][javascript][typescriptreact][typescript][json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "cSpell.words": [
    "attofil",
    "bignumber",
    "chartjs",
    "COLORBAR",
    "craco",
    "EFIL",
    "graphclient",
    "headlessui",
    "heroicons",
    "ledgerhq",
    "Orderbook",
    "picofil",
    "reduxjs",
    "Scriptable",
    "secp",
    "Subcontainer",
    "tanstack",
    "testid",
    "walletconnect",
    "zondax"
  ],
  "jest.jestCommandLine": "npm run test -- --watch"
}
