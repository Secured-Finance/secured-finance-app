# Default environment variables (committed to Git)
# For personal API keys, copy .env.local.example to .env.local
# Production values are injected via GitHub Secrets/Variables in CI/CD

# ============ Core Configuration ============

# Environment
SF_ENV=development

# Blockchain Networks (comma-separated chain IDs)
NEXT_PUBLIC_SUPPORTED_CHAIN_IDS=11155111,421614,43113,314159
NEXT_PUBLIC_NON_SUBGRAPH_SUPPORTED_CHAIN_IDS=314159

# ============ API Endpoints ============

NEXT_PUBLIC_GRAPHQL_SERVER_URL=https://dev-point-api.secured.finance/graphql
NEXT_PUBLIC_STABLECOIN_APP_URL=https://usdfc.secured.finance

# ============ Required API Keys ============

NEXT_PUBLIC_WALLET_CONNECT_ID=83209157e11b11d87ae68781c8f3762d

# ============ Feature Flags ============

NEXT_PUBLIC_USE_PACKAGE_VERSION=false
NEXT_PUBLIC_SHOW_STABLECOIN_APP_URL=true
NEXT_PUBLIC_REFERRAL_MESSAGE="Join the @Secured_Fi Points Program! ‚ú®\n\nSign up through the link below and receive 50 points as a welcome gift! Plus, enjoy a 5% boost on points earned. üéÅ\n\n%URL%?ref=%REF%\n\nComplete various quests and earn chances for future \$SFC airdrops! ü™Ç\n"

# ============ Production-Injected Variables ============
# Empty defaults allow app to run without them (features disabled)
# Production values injected via GitHub Secrets/Variables in CI/CD
# Developers can override in .env.local for enhanced local development

# RPC Providers (without these, app uses public endpoints - slower)
NEXT_PUBLIC_ALCHEMY_API_KEY=                   # Injected from secrets.ALCHEMY_API_KEY
NEXT_PUBLIC_ANKR_API_KEY=                      # Injected from secrets.ANKR_API_KEY

# Analytics (without these, no analytics tracking)
NEXT_PUBLIC_GOOGLE_ANALYTICS_TAG=              # Injected from vars.GOOGLE_ANALYTICS_TAG
NEXT_PUBLIC_AMPLITUDE_API_KEY=                 # Injected from secrets.AMPLITUDE_API_KEY

# Third-party Integrations (without these, features disabled)
NEXT_PUBLIC_SQUID_WIDGET_INTEGRATOR_ID=        # Injected from secrets.SQUID_WIDGET_INTEGRATOR_ID
NEXT_PUBLIC_SUBGRAPH_URL_314=                  # Injected from vars.SUBGRAPH_URL_314 (Filecoin testnet)