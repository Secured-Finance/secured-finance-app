  "scripts": {
    "start": "next dev",
    "build": "next build",
    "postbuild": "npm run export",
    "preview:server": "next build && next start",
    "preview": "npm run build && npx serve ./build",
    "export": "next export -o ./build",
    "build:server": "next build",
    "preexport": "rm -rf ./build",
    "test": "node --expose-gc ./node_modules/.bin/jest --watch --logHeapUsage",
